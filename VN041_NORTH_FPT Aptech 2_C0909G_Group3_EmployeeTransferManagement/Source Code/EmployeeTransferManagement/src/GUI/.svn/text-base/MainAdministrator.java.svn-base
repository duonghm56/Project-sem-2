    private void cbSearchByLocationActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_cbSearchByLocationActionPerformed
        // TODO add your handling code here:
        if (cbSearchByLocation.getSelectedIndex() == 0) {
            onLoadTableEmployee();
        } else {
            try {
                int locationID = LocationDAO.getLocationID((String) cbSearchByLocation.getSelectedItem());
                tablemodelEmpl.dataChanged();
                EmployeeDAO.getEmployeeByLocation(locationID);
            } catch (SQLException ex) {
            }
        }
    }//GEN-LAST:event_cbSearchByLocationActionPerformed

    private void cbSearchByDepartmentActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_cbSearchByDepartmentActionPerformed
        // TODO add your handling code here:
        if (cbSearchByDepartment.getSelectedIndex() == 0) {
            onLoadTableEmployee();
        } else {
            try {
                int departmentID = DepartmentDAO.getDepartmentID((String) cbSearchByDepartment.getSelectedItem());
                tablemodelEmpl.dataChanged();
                System.out.println(departmentID);
                EmployeeDAO.getEmployeeByDepartment(departmentID);
            } catch (SQLException ex) {
            }
        }
    }//GEN-LAST:event_cbSearchByDepartmentActionPerformed

