    public void loadEmployee() throws SQLException {
        Vector v = new Vector();
        v = EmployeeDAO.getNameEmployee();
        cbEmployee.setModel(new DefaultComboBoxModel(v));

    }

    public void insertTransfer() throws SQLException {
        objTransfer = new Transfer();
        objTransfer.setEmployeeID(EmployeeDAO.getEmployeeID((String) cbEmployee.getSelectedItem()));
        int transferTypeID = 1;
        objTransfer.setTransferTypeID(transferTypeID);
        objTransfer.setFromProjectID(transferTypeID);
    }

